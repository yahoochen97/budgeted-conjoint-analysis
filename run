#!/bin/bash

mkdir -p results

MAXSEED=1
declare -a data_names=("2Dplane" "Friedman")
declare -a NS=(1000)

if [ $1 = "run" ]
then
    for SEED in $(seq 1 1 $MAXSEED); do
        for data_name in ${data_names[@]}; do
            for N in ${NS[@]}; do
                bsub -G SEAS-Lab-Garnett -J "${data_name}${N}_${SEED}" "./onerun $SEED $data_name $N"
            done
        done
    done
elif [ $1 = "summary" ]
then
    for effect in ${EFFECTS[@]}; do
        Rscript --vanilla data/summary.R $MAXSEED $effect "${ULS[@]}" "${RHOS[@]}"
    done
fi